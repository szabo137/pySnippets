   "source": [
    "def norm(a0,m,dphi):\n",
    "    dphiEff = 3.0/4.0*dphi\n",
    "    alpha=1.0/137.0\n",
    "    e=np.sqrt(4.0*np.pi*alpha)\n",
    "    return a0**2*m**2/2/(e**2)*dphiEff"
   ]
